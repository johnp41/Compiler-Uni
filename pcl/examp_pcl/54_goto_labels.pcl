program labelsGoto ;

label  arxh , exodos ;

var i : integer ;

begin
    writeString("gimme an integer ");
    i := readInteger() ; 
arxh :   i := i + 3 ;
    if  i > 69 then 
                goto exodos ;
    writeInteger(i);
    writeString("\n");
    goto arxh ;

exodos :
        writeString("greater than 69 . and actually is  ");
        writeInteger(i);
end.